<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fallinggeo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/g_triangle"
        android:label="@string/app_name"
        android:roundIcon="@drawable/g_triangle"
        android:supportsRtl="true"
        android:theme="@style/Theme.FallingGeo"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity_game_infinityy"
            android:exported="false" />
        <activity
            android:name=".game.InfinityModeActivity"
            android:exported="false" />
        <activity android:name=".RegistActivity" />
        <activity
            android:name=".ui.GameMenusActivity"
            android:exported="true" />
        <activity
            android:name=".LoginActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>